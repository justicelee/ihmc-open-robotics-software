#ifndef __controller_msgs__msg__WholeBodyPoseTrajectoryMessage__idl__
#define __controller_msgs__msg__WholeBodyPoseTrajectoryMessage__idl__

#include "geometry_msgs/msg/./Pose_.idl"
module controller_msgs
{
  module msg
  {
    module dds
    {

      /**
       * This message is part of the IHMC whole-body controller API.
       * Experimental mode of specifying a desired configuration in terms of root pose and joint angles.
       */
      @TypeCode(type="controller_msgs::msg::dds_::WholeBodyPoseTrajectoryMessage_")
      struct WholeBodyPoseTrajectoryMessage
      {
        /**
         * Unique ID used to identify this message, should preferably be consecutively increasing.
         */
        unsigned long sequence_id;
        double trajectory_duration;
        sequence<double, 50> joint_angles;
        geometry_msgs::msg::dds::Pose pelvis_pose;
        long joint_name_hash;
      };
    };
  };
};

#endif
